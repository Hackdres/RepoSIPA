{% extends 'base.html.twig' %}

{% block body %}
    <h1>Municipios</h1>

    <table>
        <tbody>
            <tr>
                <th>Idmunicipio</th>
                <td>{{ municipio.idmunicipio }}</td>
            </tr>
            <tr>
                <th>Nommunicipio</th>
                <td>{{ municipio.nommunicipio }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_municipios_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_municipios_edit', { 'id': municipio.idmunicipio }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
